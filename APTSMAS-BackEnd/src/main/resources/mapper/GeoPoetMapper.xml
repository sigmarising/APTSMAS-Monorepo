<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aptsmas.dataservice.mapper.GeoPoetMapper">
    <select id="getEntitiesByGeoId" resultType="com.aptsmas.dataservice.entity.GeoPoet">
        SELECT geoId, poetId
        FROM geo_poet
        WHERE geoId = #{geoId}
    </select>
</mapper>