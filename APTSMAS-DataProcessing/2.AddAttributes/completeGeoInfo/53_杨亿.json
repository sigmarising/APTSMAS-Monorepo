                "detail": [
                    "974-978年 ，1-5岁",
                    "974年 生于玉山县。",
                    "974年 居玉山。",
                    "976年 母授小经，随即成诵。",
                    "977年 居玉山。",
                    "978年6月30日 居玉山,祖父卒。"
                ]
            },
            {
                "lat": 27.91721,
                "lng": 118.54138,
                "id": "00100700010008",
                "regionId": "00100700010008",
                "location": "浦城",
                "city": "南平市",
